doctype html
html
  head
    title= title
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
    script(src='/javascripts/socket.io-client/socket.io.js')
    script(src='/javascripts/vote.js')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/votingclient.css')
  body(onload='connect()')
    h1 #{title}
    div: p(id='status') Waiting for input
    button(id='vote' onClick='vote(1)' class='myButton') #{button1}
    br
    button(id='vote' onClick='vote(2)' class='myButton') #{button2}
    br
    button(id='vote' onClick='vote(3)' class='myButton') #{button3}
    br
    button(id='vote' onClick='vote(4)' class='myButton') #{button4}
    br
    button(id='vote' onClick='vote(5)' class='myButton') #{button5}

